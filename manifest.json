{
  "name": "PrivacyMI5",
  "description": "An app for monitoring the data you exchange with websites.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "html/hello.html",
    "default_icon": "assets/img/logo.png"
  },
  "content_scripts": [
    {
      "js": [
        "js/lib.js",
        "js/content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}